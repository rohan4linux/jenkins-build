{
        "family": "mytaskdef",
        "networkMode": "awsvpc",
        "cpu": "256",
        "memory": "512"
        "execution_role_arn": "arn:aws:iam::788967628378:role/ecsTaskExecutionRole",
        "containerDefinitions": [{
                "essential": true,
                "name": "hello-world",
                "image": "788967628378.dkr.ecr.us-east-1.amazonaws.com/hello-world:latest",
                "portMappings": [{
                        "containerPort": 80,
                        "hostPort": 80,
                        "protocol": "tcp"
                }]
        }],
        "requiresCompatibilities": [
                "FARGATE"
        ]
}
