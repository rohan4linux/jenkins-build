{
	"family": "mytaskdef",
	"networkMode": "awsvpc",
	"cpu": "256",
	"memory": "512",
	"containerDefinitions": [{
		"essential": true,
		"name": "hello-jenkins",
		"image": "public.ecr.aws/s5q0x2a7/hello-jenkins:v_$BUILD_NUMBER",
		"portMappings": [{
			"containerPort": 80,
			"hostPort": 80,
			"protocol": "tcp"
		}]
	}],
	"requiresCompatibilities": [
		"FARGATE"
	]
}
